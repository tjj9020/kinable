{
  "name": "@kinable/chat-api-service",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild:sam": "rm -rf node_modules/@kinable/common-types && mkdir -p node_modules/@kinable && cp -R ../../packages/common-types/dist node_modules/@kinable/common-types && cp ../../packages/common-types/package.json node_modules/@kinable/common-types/",
    "build": "tsc -p tsconfig.build.json",
    "build:sam": "npm run prebuild:sam && sam build -t sam.yaml",
    "deploy": "./build-and-deploy.sh",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=\\\\\\.integration\\\\\\.test\\\\\\.ts$",
    "test:integration": "jest --testPathPattern=\\\\\\.integration\\\\\\.test\\\\\\.ts$",
    "test:e2e": "node src/integration-tests/fullChatFlow.integration.test.mjs",
    "lint": "eslint . --ext .ts --quiet"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.808.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.808.0",
    "@types/aws-lambda": "^8.10.0",
    "@types/jest": "^29.0.0",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "node-fetch": "^2.7.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.50.4",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-secrets-manager": "^3.808.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "@kinable/common-types": "file:../../packages/common-types",
    "amazon-cognito-identity-js": "^6.3.7",
    "aws-jwt-verify": "^4.0.0",
    "dotenv": "^16.5.0",
    "openai": "^4.98.0"
  },
  "keywords": [],
